<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.qantas">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_plane_icon_foreground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_plane_icon_foreground"
        android:theme="@style/Theme.Qantas"
        tools:targetApi="33">
        <activity
            android:name=".view.AirportListActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.AirportDetailsActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>